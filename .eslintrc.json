{
  "parser": "@babel/eslint-parser",
  "env": {
    "browser": false,
    "es6": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "plugins": ["@babel"],
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": [2, { "args": "all", "argsIgnorePattern": "^_+" }],
    "no-unused-expressions": "off",
    "class-methods-use-this": "off"
  }
}
